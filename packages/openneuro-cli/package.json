{
  "name": "openneuro-cli",
  "version": "3.30.0",
  "description": "OpenNeuro command line tools.",
  "main": "index.js",
  "repository": "git@github.com:OpenNeuroOrg/openneuro.git",
  "engines": {
    "node": ">=12.9.0"
  },
  "os": [
    "!win32"
  ],
  "author": "Squishymedia",
  "license": "MIT",
  "bin": {
    "git-annex-remote-openneuro": "./src/index.js",
    "git-credential-openneuro": "./src/index.js",
    "openneuro": "./src/index.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.12",
    "bids-validator": "1.6.2",
    "cli-progress": "^3.8.2",
    "colors": "^1.4.0",
    "commander": "^2.15.1",
    "cross-fetch": "^3.1.2",
    "elastic-apm-node": "^3.12.1",
    "esm": "^3.0.16",
    "fetch-h2": "^2.5.1",
    "find-config": "^1.0.0",
    "inquirer": "^5.2.0",
    "jwt-decode": "^3.1.2",
    "mkdirp": "^0.5.1",
    "openneuro-client": "^3.30.0"
  },
  "scripts": {
    "build": "tsc -b",
    "openneuro": "node src/index.js",
    "git-credential-openneuro": "node src/index.js",
    "git-annex-remote-openneuro": "node src/index.js"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!bids-validator).+\\.js$"
    ]
  },
  "devDependencies": {
    "@types/colors": "^1",
    "jest-fetch-mock": "^3.0.3",
    "metro-memory-fs": "0.61.0"
  }
}
